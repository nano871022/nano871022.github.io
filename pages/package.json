{
  "name": "pages",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href /nano871022.github.io/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:i18n": "ng build --configuration production --base-href /nano871022.github.io/",
    "build:en": "ng build --configuration production --base-href /nano871022.github.io/ --localize en-US",
    "build:es": "ng build --configuration production --base-href /nano871022.github.io/es/ --localize es-ES",
    "serve:en": "ng serve --configuration=development --host 0.0.0.0 --port 4200 --open --locale en-US",
    "serve:es": "ng serve --configuration=development --host 0.0.0.0 --port 4201 --open --locale es-ES",
    "serve:ssr:pages": "node dist/pages/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.1",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/localize": "^20.0.2",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
